{
  "name": "purescript-outwatch",
  "version": "0.6.0",
  "description": "A reactive and functional UI Library",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "rxjs": "^5.2.0",
    "snabbdom": "^0.6.6"
  },
  "devDependencies": {
    "bower": "^1.8.0",
    "phantomjs": "^2.1.7",
    "pulp": "^10.0.1",
    "purescript": "^0.10.7",
    "rimraf": "^2.6.1",
    "webpack": "^2.3.2",
    "webpack-dev-server": "^2.4.2"
  },
  "scripts": {
    "postinstall": "bower cache clean && bower install",
    "test": "pulp browserify -I test -m Test.Main -t index.js && phantomjs index.js",
    "clean:output": "./node_modules/.bin/rimraf output",
    "build:example-counter": "pulp browserify --include examples/src --to examples/dist/app.js --main Example.Counter",
    "build:example-counter-store": "pulp browserify --include examples/src --to examples/dist/app.js --main Example.CounterStore",
    "build:example-bmi-calculator": "pulp browserify --include examples/src --to examples/dist/app.js --main Example.BMICalculator"
  },
  "keywords": [
    "purescript"
  ],
  "author": "Luka Jacobowitz",
  "license": "Apache-2.0"
}
